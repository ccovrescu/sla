<!-- ICR PRINT -->
<div class="row">
    <div class="col-lg-12">
        <div class="icr-logo">
            <img src="https://www.teletrans.ro/img/logo_old.png" width="200px"/>
        </div>
    </div>	
    {# introdus 04.03.2019 #}
    {# sfarsit introdus 04.03.2019 #}
</div>

<div class="row">
    <div class="col-lg-12">
        <h2 class="text-center icr-title">FIȘĂ DE INTERVENȚIE<sup>1</sup></h2>
    </div>
</div>


<div class="main">
<div class="row">
    <div class="item service-type top-cell left-cell text-center">
        Departamentul TELETRANS (DTI/DTC/DIP)<br/>
        <abbr title="attribute">
            {% if ticket.equipment is defined and ticket.equipment is not null %}
                {% if ticket.equipment.service.department.id == "1" %}
                    {{ "DIP"|str_pad(10)|replace({' ':'&nbsp;'})|raw }}
                {% elseif ticket.equipment.service.department.id == "2" %}
                    {{ "DTC"|str_pad(10)|replace({' ':'&nbsp;'})|raw }}
                {% elseif ticket.equipment.service.department.id == "3" %}
                    {{ "DTI"|str_pad(10)|replace({' ':'&nbsp;'})|raw }}
                {% endif %}
            {% else %}
                {{ " "|str_pad(10)|replace({' ':'&nbsp;'})|raw }}
            {% endif %}
        </abbr>
    </div>
    <div class="item agency-center top-cell text-center">
        Entitate TELETRANS (Executiv/Agentie/Centru)<br/>
        <abbr title="attribute">
            {% if ticket is not null %}
                {{ ticket.ticketAllocations.first.branch.name|str_pad(60)|replace({' ':'&nbsp;'})|raw }}
            {% else %}
                {{ " "|str_pad(60)|replace({' ':'&nbsp;'})|raw }}
            {% endif %}
        </abbr>
        <br/>&nbsp;
    </div>
    <div class="item emergent top-cell right-cell text-center">
        <br/>
        Urgent:
        {% if ticket.equipment is defined and ticket.equipment is not null %}
            {% if ticket.emergency is not null and ticket.emergency.id == 1 %}
                DA <i class="fa fa-square-o"></i> &nbsp; NU <i class="fa fa-check-square-o"></i>
            {% else %}
                DA <i class="fa fa-check-square-o"></i> &nbsp; NU <i class="fa fa-square-o"></i>
            {% endif %}
        {% else %}
            DA <i class="fa fa-square-o"></i> &nbsp;NU <i class="fa fa-square-o"></i>
        {% endif %}
        <br/>&nbsp;
    </div>
</div>

<div class="row">
    <div class="item ticket-no left-cell text-center">
        Nr. SLA:
        <abbr title="attribute">
            {{ ticket.id|default(" ")|str_pad(10)|replace({' ':'&nbsp;'})|raw }}
        </abbr>
        <br/>&nbsp;
    </div>
    <div class="item announced-at text-center">
        Data/Ora sesizarii:<br/>
        <abbr title="attribute">
            {{ (ticket is not null ? ticket.announcedAt|date('d.m.Y H:i') : " ")|str_pad(35)|replace({' ':'&nbsp;'})|raw }}
        </abbr>
    </div>
    <div class="item taken-by text-center">
        Persoana din tura care preia sesizarea:<br/>
        <abbr title="attribute">
            {{ticket.takenBy|default(" ")|str_pad(70)|replace({' ':'&nbsp;'})|raw }}
        </abbr>
    </div>
</div>

<div class="row">
    <div class="item transmission-type left-cell text-center">
        Mod de transmitere sesizare:<br/>

        {% set transmission = '(' %}
        {% for tType in transmissionTypes %}
            {% if loop.first %}
                {% set transmission = '(' ~ ((ticket is null or tType==ticket.transmissionType) ? tType.name|capitalize : '<strike>' ~ tType.name|capitalize ~ '</strike>') %}
            {% else %}
                {% set transmission = transmission ~ '/' ~ (( ticket is null or tType==ticket.transmissionType) ? tType.name|capitalize : '<strike>' ~ tType.name|capitalize ~ '</strike>') %}
            {% endif %}
        {% endfor %}

        {{ (transmission ~ ')')|raw }}
    </div>
    <div class="item announced-to right-cell text-center">
        Persoana anuntata:<br/>
        <abbr title="attribute">
            {{ ticket.announcedTo|default(" ")|str_pad(70)|replace({' ':'&nbsp;'})|raw }}
        </abbr>
    </div>
</div>

<div class="row">
    <div class="item launched-by left-cell text-center">
        Sesizare lansata de: (Ex/DEN/DET/ST/OMEPA)<br/>
        <abbr title="attribute">
            {{ ticket.equipment.owner.name|default(" ")|str_pad(70)|replace({' ':'&nbsp;'})|raw }}
        </abbr>
    </div>
    <div class="item announced-by right-cell text-center">
        Persoana care face sesizarea:<br/>
        <abbr title="attribute">
            {{ ticket.announcedBy|default(" ")|str_pad(70)|replace({' ':'&nbsp;'})|raw }}
        </abbr>
    </div>
</div>

<div class="row">
    <div class="item description left-cell right-cell">
        Descriere pe scurt deranjament:
                <pre>
                    {{ ticket.description|default('') }}
                </pre>
    </div>
</div>

<div class="row">
    <div class="item is-real left-cell">
        Sesizarea este reala:
        {% if ticket.isReal is defined and ticket.isReal is not null %}
            {% if ticket.isReal == 1 %}
                DA <i class="fa fa-check-square-o"></i> &nbsp; NU <i class="fa fa-square-o"></i>
            {% else %}
                Da <i class="fa fa-square-o"></i> &nbsp; Nu <i class="fa fa-check-square-o"></i>
            {% endif %}
        {% else %}
            DA <i class="fa fa-square-o"></i> &nbsp; NU <i class="fa fa-square-o"></i>
        {% endif %}
    </div>
    <div class="item not-real-reason right-cell">
        De ce NU este reala:
                <pre>
                    {{ ticket.notRealReason|default('') }}
                </pre>
    </div>
</div>

<div class="row">
    <div class="item ticket-type left-cell text-center">
        Tip interventie:<br/>
        <abbr title="attribute">
            {{ ticket.ticketType.name|default(' ')|str_pad(50)|replace({' ':'&nbsp;'})|raw }}
        </abbr>
    </div>

    <div class="item compartment right-cell text-center">
        Compartimentul din cadrul TELETRANS implicat in rezolvarea sesizarii:<br/>
        <abbr title="attribute">
            {% if ticket.equipment is defined and ticket.equipment is not null %}
                {{ticket.compartment|default(' ')|str_pad(70)|replace({' ':'&nbsp;'})|raw }}
            {% else %}
                {% if ticket.isReal is defined and ticket.isReal is not null  %}
                    {{ ticket.compartment|default(' ')|str_pad(70)|replace({' ':'&nbsp;'})|raw }}
                {% else  %}
                    {{ ' '|str_pad(70)|replace({' ':'&nbsp;'})|raw  }}
                {% endif %}
            {% endif %}
        </abbr>
    </div>
</div>

<div class="row">
    <div class="item oldness left-cell">
        Vechime echipament:
        {% if ticket.isReal is defined and ticket.isReal is not null %}
            {% if ticket.oldness.id == 2 %}
                <i class="fa fa-square-o"></i> 1-3 ani&nbsp;
                <i class="fa fa-check-square-o"></i> peste 3 ani
            {% else %}
                <i class="fa fa-check-square-o"></i> 1-3 ani&nbsp;
                <i class="fa fa-square-o"></i> peste 3 ani
            {% endif %}
        {% else %}
            <i class="fa fa-square-o"></i> 1-3 ani&nbsp;
            <i class="fa fa-square-o"></i> peste 3 ani
        {% endif %}
    </div>
    <div class="item backup-solution right-cell">
        S-a asigurat solutie de rezerva:
        {% if ticket.isReal is defined and ticket.isReal is not null %}
            {# 1: Da #}
            {% if ticket.backupSolution.id == 1 %}
                <i class="fa fa-check-square-o"></i> Da&nbsp;
                <i class="fa fa-square-o"></i> Nu&nbsp;
                <i class="fa fa-square-o"></i> Nu este cazul
            {% endif %}

            {# 2: NU #}
            {% if ticket.backupSolution.id == 2 %}
                <i class="fa fa-square-o"></i> Da&nbsp;
                <i class="fa fa-check-square-o"></i> Nu&nbsp;
                <i class="fa fa-square-o"></i> Nu este cazul
            {% endif %}

            {# 3: Nu este cazul#}
            {% if ticket.backupSolution.id == 3 %}
                <i class="fa fa-square-o"></i> Da&nbsp;
                <i class="fa fa-square-o"></i> Nu&nbsp;
                <i class="fa fa-check-square-o"></i> Nu este cazul
            {% endif %}
        {% else %}
            <i class="fa fa-square-o"></i> Da&nbsp;
            <i class="fa fa-square-o"></i> Nu&nbsp;
            <i class="fa fa-square-o"></i> Nu este cazul
        {% endif %}
    </div>
</div>

<div class="row">
    <div class="item equipment left-cell right-cell">
        Echipament:
        <abbr title="attribute">
            <em>{{ ticket.equipment.name|default(' ')|str_pad(140, ' ', 1)|replace({' ':'&nbsp;'})|raw }}</em>
        </abbr>
    </div>
</div>

<div class="row">
    <div class="item affected-systems left-cell right-cell">
        Sisteme afectate:
        {% set systems = '' %}
        {% if ticket.equipment is defined and ticket.equipment is not null %}
            {% for tm in ticket.ticketMapping %}
                {% if not loop.first %}
                    {% set systems = systems ~ ', ' ~ tm.mapping.system.name %}
                {% else %}
                    {% set systems = systems ~ tm.mapping.system.name %}
                {% endif %}
            {% endfor %}
        {% endif %}
        <pre>
                    {{ systems }}
                </pre>
    </div>
</div>

<div class="row">
    <div class="item fixed-by left-cell">
        Persoana care rezolva:
        <abbr title="attribute">
            {% if ticket.equipment is defined and ticket.equipment is not null %}
                {{ ticket.fixedBy|default(' ')|str_pad(50, ' ', 1)|replace({' ':'&nbsp;'})|raw }}
            {% else %}
                {% if ticket.isReal is defined and ticket.isReal is not null  %}
                    {{ ticket.fixedBy|default(' ')|str_pad(50, ' ', 1)|replace({' ':'&nbsp;'})|raw }}
                {% else %}
                    {{ ' '|str_pad(50)|replace({' ':'&nbsp;'})|raw  }}
                {% endif %}
            {% endif %}
        </abbr>
    </div>
    <div class="item fixed-at right-cell">
        Data/Ora rezolvarii:
        <abbr title="attribute">
            {% if ticket.equipment is defined and ticket.equipment is not null %}
                {{ (ticket.fixedAt is not null ? ticket.fixedAt|date('d.m.Y H:i') : " ")|str_pad(30)|replace({' ':'&nbsp;'})|raw }}
            {% else %}
                {% if ticket.isReal is defined and ticket.isReal is not null  %}
                    {{ (ticket.fixedAt is not null ? ticket.fixedAt|date('d.m.Y H:i') : " ")|str_pad(30)|replace({' ':'&nbsp;'})|raw }}
                {% else %}
                    {{ ' '|str_pad(30)|replace({' ':'&nbsp;'})|raw  }}
                {% endif %}
            {% endif %}
        </abbr>
    </div>
</div>

<div class="row">
    <div class="item fixed-mode left-cell right-cell">
        Mod de rezolvare:
                <pre>
                    {% if ticket is not null %}
                        {{ ticket.fixedMode }}
                    {% endif %}
                </pre>
    </div>
</div>

<!-- last row -->
<div class="row">
    <div class="item resources left-cell right-cell bottom-cell">
        Resurse utilizate:
                <pre>
                    {% if ticket is not null %}
                        {{ ticket.resources }}
                    {% endif %}
                </pre>
    </div>
</div>
</div>

